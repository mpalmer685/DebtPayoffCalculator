{
  "name": "debt-payoff-calculator",
  "version": "0.0.1",
  "description": "Debt Payoff Calculator",
  "main": "index.js",
  "scripts": {
    "test": "better-npm-run test",
    "coverage": "better-npm-run coverage",
    "start": "node server.js"
  },
  "betterScripts": {
    "test": {
      "command": "mocha --compilers js:babel-core/register --require test/test_setup.js --colors --recursive test/",
      "env": {
        "NODE_PATH": "./source"
      }
    },
    "coverage": {
      "command": "babel-node ./node_modules/.bin/isparta cover node_modules/mocha/bin/_mocha -- --compilers js:babel-core/register --require test/test_setup.js --colors --recursive test/",
      "env": {
        "NODE_PATH": "./source"
      }
    }
  },
